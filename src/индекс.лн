константа сервис = {
  создать(данные, параметры) {
    консоль.сообщить('ВЫЗВАН МЕТОД СОЗДАТЬ', параметры)
    вернуть { ид: `${Математика.пол(Математика.случайно() * (данные.макс - данные.мин)) + данные.мин}` }
  },
  обновить(ид, данные, параметры) {
    консоль.предупредить('ВЫЗВАН МЕТОД ОБНОВИТЬ', параметры)
  },
  удалить(ид, данные, параметры) {
    консоль.ошибка('ВЫЗВАН МЕТОД УДАЛИТЬ', параметры)
  },
}

функция ф1({ данные, параметры }) {
  консоль.сообщить('ф1 начинается здесь')

  переменная штука = сервис.создать({ ...данные, а: 1 })

  консоль.сообщить(`ИД: ${штука.ид}`)

  сервис.обновить(штука.ид, { б: 2 })

  сервис.удалить(штука.ид)

  консоль.сообщить('ф1 заканчивается здесь')
}

ф1({ данные: { мин: 120, макс: 283 } })